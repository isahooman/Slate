{
  "name": "slate",
  "version": "4.12",
  "description": "A Discord.js Bot Startup Codebase",
  "keywords": [
    "node",
    "discord",
    "discord.js"
  ],
  "homepage": "https://github.com/isahooman/slate#readme",
  "scripts": {
    "tree": "pwsh ./.vscode/scripts/Project_Tree.ps1",
    "start": "cross-os start-os",
    "gui": "electron gui/main/main.js",
    "start-os": {
      "darwin": "sh start.sh",
      "linux": "sh start.sh",
      "win32": "start.bat"
    },
    "build": "npm install",
    "prestart": "npm install cross-os"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "discord.js": "^14.11.0",
    "json5": "^2.2.3",
    "mathjs": "^14.4.0",
    "moment": "^2.30.1",
    "moment-duration-format": "^2.3.2",
    "node-fetch": "^3.3.2",
    "node-os-utils": "^1.3.7",
    "@xterm/xterm": "^5.3.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-search": "^0.15.0"
  },
  "devDependencies": {
    "cross-os": "^1.5.0",
    "electron": "^35.1.3",
    "eslint": "^9.23.0",
    "eslint-plugin-jsdoc": "^48.0.6"
  },
  "engines": {
    "node": ">=16.20.2",
    "npm": ">=8.19.4"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
