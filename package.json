{
  "name": "slate",
  "version": "4.12",
  "description": "A Discord.js Bot Startup Codebase",
  "keywords": [
    "node",
    "discord",
    "discord.js"
  ],
  "homepage": "https://github.com/isahooman/slate#readme",
  "scripts": {
    "tree": "pwsh ./.vscode/scripts/Project_Tree.ps1",
    "start": "cross-os start-os",
    "start-os": {
      "darwin": "sh start.sh",
      "linux": "sh start.sh",
      "win32": "start.bat"
    },
    "build": "npm install",
    "prestart": "npm install cross-os",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "discord.js": "^14.11.0",
    "json5": "^2.2.3",
    "mathjs": "^13.0.2",
    "moment": "^2.29.4",
    "moment-duration-format": "^2.3.2",
    "node-os-utils": "^1.3.7"
  },
  "devDependencies": {
    "cross-os": "^1.5.0",
    "electron": "28.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-jsdoc": "^48.0.6"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.3"
  },
  "engines": {
    "node": ">=16.20.2",
    "npm": ">=8.19.4"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
