{   
  "name": "slate",
  "version": "4.8",
  "description": "A Discord.js Bot Startup Codebase",
  "keywords":[
    "node",
    "discord",
    "discord.js"
  ],
  "homepage": "https://github.com/isahooman/slate#readme",
  "scripts": {
    "start": "cross-os start-os",
    "start-os": {
      "darwin": "sh start.sh",
      "linux": "sh start.sh",
      "win32": "start.bat"
    },
    "prestart": "npm install --only=dev",
    "build": "npm install",
    "test": "eslint . --ignore-pattern node_modules/"
  },
  "dependencies": {
    "canvas": "^2.11.2",
    "chalk": "^4.1.2",
    "discord.js": "^14.11.0",
    "dotenv": "^16.1.4",
    "moment": "^2.29.4",
    "moment-duration-format": "^2.3.2",
    "node-fetch": "^3.3.2",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "cross-os": "^1.3.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.3"
  },
  "engines":{
    "node": ">=16.20.2",
    "npm": ">=8.19.4"
  },
  "os": ["darwin", "linux", "win32"]
}
